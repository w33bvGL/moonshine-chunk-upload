{
    "name": "w33bvgl/moonshine-chunk-upload",
    "description": "Resumable chunk upload field for MoonShine admin panel",
    "keywords": [
        "laravel",
        "moonshine",
        "chunk-upload",
        "resumable-js"
    ],
    "type": "library",
    "homepage": "https://github.com/w33bvGL",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/w33bvGL/moonshine-chunk-upload/issues",
        "source": "https://github.com/w33bvGL/moonshine-chunk-upload"
    },
    "authors": [
        {
            "name": "w33bvgl",
            "email": "w33bvgl@gmail.com",
            "homepage": "https://github.com/w33bvGL"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "pion/laravel-chunk-upload": "^1.5",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "rector/rector": "^1.0",
        "moonshine/moonshine": "4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "W33bvgl\\MoonShineChunkUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "W33bvgl\\MoonShineChunkUpload\\Tests\\": "tests/",
            "W33bvgl\\MoonShineChunkUpload\\Database\\Factories\\": "database/factories/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "conflict": {
        "moonshine/moonshine": "<3.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "rector": "vendor/bin/rector process --clear-cache",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoonshineChunkUploadServiceProvider"
            ]
        }
    }
}